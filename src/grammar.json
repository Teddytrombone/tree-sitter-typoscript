{
  "name": "TypoScript",
  "rules": {
    "typoscript": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_block_item"
          }
        ]
      }
    },
    "_block_item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\s*\\n"
        },
        {
          "type": "SYMBOL",
          "name": "modification_line"
        },
        {
          "type": "SYMBOL",
          "name": "reference_line"
        },
        {
          "type": "SYMBOL",
          "name": "assignment_line"
        },
        {
          "type": "SYMBOL",
          "name": "deletion_line"
        },
        {
          "type": "SYMBOL",
          "name": "copy_line"
        },
        {
          "type": "SYMBOL",
          "name": "configuration_block"
        },
        {
          "type": "SYMBOL",
          "name": "_imports"
        },
        {
          "type": "SYMBOL",
          "name": "condition"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "single_line_comment"
        }
      ]
    },
    "assignment_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "value"
                },
                {
                  "type": "SYMBOL",
                  "name": "multiline_value"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "deletion_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_comments"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "copy_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          },
          "named": true,
          "value": "copy_identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_comments"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "reference_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "PATTERN",
          "value": "=\\s*<"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          },
          "named": true,
          "value": "reference_identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_comments"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "modification_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ":="
        },
        {
          "type": "SYMBOL",
          "name": "modifier_function"
        },
        {
          "type": "SYMBOL",
          "name": "modifier_parameters"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_comments"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "configuration_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "block"
        }
      ]
    },
    "condition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "PATTERN",
          "value": "[^\\]]*"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "block": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "{"
          },
          "named": true,
          "value": "block_punctuation"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_block_item"
          }
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "}"
          },
          "named": true,
          "value": "block_punctuation"
        }
      ]
    },
    "constant": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{$"
        },
        {
          "type": "PATTERN",
          "value": "[a-zA-Z0-9_\\-\\\\\\.]+"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "value": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "[^\\n]"
          },
          {
            "type": "SYMBOL",
            "name": "constant"
          }
        ]
      }
    },
    "multiline_value": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT1",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "constant"
                  },
                  {
                    "type": "PATTERN",
                    "value": "[^\\)]"
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "((?:\\.)|(?:[a-zA-Z0-9_\\-\\\\]+(?:\\.[a-zA-Z0-9_\\-\\\\]*)*))"
    },
    "modifier_function": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_\\-\\\\]+"
    },
    "modifier_parameters": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "modifier_parameter"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "modifier_parameter"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "modifier_parameter": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "constant"
          },
          {
            "type": "PATTERN",
            "value": "[^,\\)]"
          }
        ]
      }
    },
    "_comments": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "single_line_comment"
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "/*"
          },
          {
            "type": "PATTERN",
            "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
          },
          {
            "type": "STRING",
            "value": "/"
          }
        ]
      }
    },
    "single_line_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "#"
              },
              {
                "type": "STRING",
                "value": "//"
              }
            ]
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "REPEAT1",
                  "content": {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "constant"
                      },
                      {
                        "type": "PATTERN",
                        "value": "[^\\\"]"
                      }
                    ]
                  }
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "'"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "REPEAT1",
                  "content": {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "constant"
                      },
                      {
                        "type": "PATTERN",
                        "value": "[^\\']"
                      }
                    ]
                  }
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "STRING",
              "value": "'"
            }
          ]
        }
      ]
    },
    "_imports": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "import_legacy"
        },
        {
          "type": "SYMBOL",
          "name": "import"
        }
      ]
    },
    "import_legacy": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "STRING",
          "value": "INCLUDE_TYPOSCRIPT"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "PATTERN",
          "value": "\\s*"
        },
        {
          "type": "STRING",
          "value": "source="
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "\\s+"
                },
                {
                  "type": "ALIAS",
                  "content": {
                    "type": "SYMBOL",
                    "name": "modifier_function"
                  },
                  "named": true,
                  "value": "condition_attribute"
                },
                {
                  "type": "STRING",
                  "value": "="
                },
                {
                  "type": "SYMBOL",
                  "name": "string"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "import": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@import"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[\\t ]"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

